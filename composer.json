{
    "name": "hut6/oembed",
    "description": "A simple plugin to extract media information from websites, like youtube videos, twitter statuses or blog articles.",
    "type": "craft-plugin",
    "version": "2.0.7",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "oembed",
        "iframe",
        "youtube",
        "instagram",
        "twitter",
        "facebook"
    ],
    "support": {
        "docs": "https://github.com/hut6/oembed/blob/master/README.md",
        "issues": "https://github.com/hut6/oembed/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "reganlawton",
            "homepage": "https://github.com/reganlawton"
        },
        {
            "name": "hutsix",
            "homepage": "https://hutsix.com.au"
        }
    ],
    "require": {
        "craftcms/cms": "~3.0.0-beta.23",
        "essence/essence": "^3.3"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
          "hut6\\oembed\\": "src/"
        }
    },
    "extra": {
        "name": "oEmbed",
        "handle": "oembed",
        "schemaVersion": "2.0.0",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/hut6/oembed/master/CHANGELOG.md",
        "components": {
            "oembedService": "hut6\\oembed\\services\\OembedService"
        },
        "class": "hut6\\oembed\\Oembed"
    }
}
